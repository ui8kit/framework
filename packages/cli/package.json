{
  "name": "ui8kit",
  "version": "1.1.0",
  "description": "A CLI for adding UI components to your Vite React projects (UI8Kit core/blocks registries)",
  "type": "module",
  "bin": {
    "ui8kit": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "node ../../scripts/smoke-parity.mjs"
  },
  "dependencies": {
    "@ui8kit/sdk": "workspace:*",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "execa": "^8.0.1",
    "fs-extra": "^11.1.1",
    "glob": "^10.3.10",
    "node-fetch": "^3.3.2",
    "ora": "^7.0.1",
    "prompts": "^2.4.2",
    "typescript": "^5.2.2",
    "zod": "^3.22.4",
    "zod-to-json-schema": "^3.24.5"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.8.0",
    "@types/prompts": "^2.4.9",
    "tsup": "^7.2.0"
  },
  "keywords": [
    "cli",
    "ui",
    "components",
    "vite",
    "react",
    "shadcn",
    "tailwind",
    "ui8kit",
    "core"
  ],
  "author": {
    "name": "ui8kit",
    "url": "https://github.com/ui8kit"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ui8kit/cli.git",
    "directory": "packages/cli"
  },
  "homepage": "https://ui.buildy.tw",
  "engines": {
    "node": ">=18.0.0"
  }
}
