{
  "name": "@ui8kit/render",
  "version": "0.1.0",
  "description": "React static renderer for UI8Kit - converts React components to HTML with data-class attributes",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target node",
    "dev": "bun --watch src/index.ts",
    "test": "bun test",
    "lint": "bunx tsc --noEmit",
    "typecheck": "bunx tsc --noEmit"
  },
  "dependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "bun": "^1.3.0",
    "typescript": "^5.8.0",
    "@types/bun": "^1.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0"
  }
}
