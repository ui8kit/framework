+              name
+            }
+          }
+          tags {
+            nodes {
+              id
+              tagId
+              name
+            }
+          }
+          featuredImage {
+            node {
+              id
+              sourceUrl
+              altText
+              caption
+            }
+          }
+          postFields {
+            key
+            value
+          }
+        }
+      }
+    }
+  `;
+
+  try {
+    const response = await fetch(GRAPHQL_ENDPOINT, {
+      method: 'POST',
+      headers: {
+        'Content-Type': 'application/json',
