+        {label}
+      </Text>
+      {links.map((link) => (
+        <Link
+          key={link.href}
+          to={link.href}
+          className={
+            link.active
+              ? 'text-sm px-2 py-1 rounded bg-accent font-medium'
+              : 'text-sm px-2 py-1 rounded hover:bg-accent'
+          }
+          data-class="dash-sidebar-link"
