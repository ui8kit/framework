+async function fetchPostsFromAPI(): Promise<GraphQLPost[]> {
+  const query = `
+    {
+      posts(first: 50) {
+        nodes {
+          id
+          postId
+          date
+          title
+          content
+          excerpt
+          categories {
+            nodes {
