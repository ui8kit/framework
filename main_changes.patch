+  if (!ts.isCallExpression(expr)) return false;
+  if (ts.isIdentifier(expr.expression)) return expr.expression.text === "cva";
+  if (ts.isPropertyAccessExpression(expr.expression)) return expr.expression.name.text === "cva";
+  return false;
