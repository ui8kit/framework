+ * Convert camelCase/PascalCase to kebab-case
+ */
+function kebabCase(input: string): string {
+  return input
+    .replace(/([a-z0-9])([A-Z])/g, "$1-$2")
+    .replace(/_/g, "-")
+    .toLowerCase();
+}
+
+/**
+ * Split a class string into individual tokens
+ */
+function splitTokens(s: string): string[] {
+  return s
+    .trim()
+    .split(/\s+/g)
+    .map((t) => t.trim())
+    .filter(Boolean);
+}
