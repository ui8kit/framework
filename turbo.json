{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "test": {
      "dependsOn": ["build"],
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "html": {
      "dependsOn": ["build"],
      "outputs": ["examples/**"]
    },
    "preview": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    }
  }
}
